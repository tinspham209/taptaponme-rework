{
  "typescript.tsdk": "node_modules\\typescript\\lib",
  "files.exclude": {
    "node_modules/": true
  },
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.organizeImports": true
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.tabSize": 2,
  "cSpell.words": [
    "commitlint",
    "taptaponme",
    "Toastify",
    "toastify",
    "formik",
    "mui-datatables",
    "datepicker",
    "webadmin",
    "SwipeableViews",
    "TaptapOn"
  ]
}
